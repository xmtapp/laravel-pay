{
    "name": "xmtapp/laravel-pay",
    "description": "laravel pay payment",
    "license": "Apache License 2.0",
    "authors": [
        {
            "name": "董垚",
            "email": "dong_yao_cn@163.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "XmtApp\\Payment\\": "src/"
        },
        "classmap": [
            "alipaySdk"
        ]
    },
    "extra": {
